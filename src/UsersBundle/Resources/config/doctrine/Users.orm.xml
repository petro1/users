<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UsersBundle\Entity\Users" table="users">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="birthdate" type="date" column="birthdate" nullable="false"/>
    <field name="firstName" type="string" column="first_name" length="200" nullable="false"/>
    <field name="lastName" type="string" column="last_name" length="200" nullable="false"/>
    <field name="sex" type="string" column="sex" length="1" nullable="false"/>
    <field name="registration" type="date" column="registration" nullable="false"/>
  </entity>
</doctrine-mapping>
